void stock :: trade_trend(){
 
 profit = 0; 
 double change = 0; 
 
 for(int i = 2; i<64; i++)
 {      
    change = (price[i-1]-price[i-2])/(price[i-2]);
    
    //cout<<change<<", ";
    
     if(change>0){
         profit += (price[i]-price[i-1])/price[i-1]; //long
     }
     
     if(change<0){
         profit += (price[i-1]-price[i])/price[i-1]; //short
     }
     //cout<<profit<<"\n";
 }
    
}

void stock :: trade_volume(){
    
    profit = 0; 
    double change = 0; 
    double factor = 0; 
    
    for(int i = 2; i<64; i++) { 
        
        change = volume[i-1]-volume[i-2];
        
       if(change>0){
         profit += (price[i]-price[i-1])/price[i-1]; //long
         }
     
     if(change<0){
         profit += (price[i-1]-price[i])/price[i-1]; //short
        }
        
    }
    
}

int main()
{
    stock Apple; 
    Apple.trade_volume(); 
    cout<<Apple.profit;
    return 0;    
}
