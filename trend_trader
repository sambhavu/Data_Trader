void stock :: flipdata(){
    for(int i = 0; i<64; i++){
     new_data[i] = price_data[63-i];
    }
}

void stock :: trade(){
 
 double profit = 0; 
 double change = 0; 
 
 for(int i = 2; i<64; i++)
 {      
    change = new_data[i-1]-new_data[i-2]/(new_data[i-2]);
    
     if(change>0){
         profit += (new_data[i]-new_data[i-1])/new_data[i-1];
     }
     
     if(change<0){
         profit += (new_data[i-1]-new_data[i])/new_data[i-1];
     }
     cout<<profit<<"\n";
 }
    
}

int main()
{
    stock Apple; 
    Apple.flipdata();
    Apple.trade(); 
    
    return 0;    
}
